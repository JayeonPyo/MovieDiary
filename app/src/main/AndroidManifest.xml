<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.internet">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/browser"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Baro.git_webview"></activity>
        <activity android:name=".Baro.facebook_webview" />
        <activity android:name=".Baro.daum_webview" />
        <activity android:name=".Baro.google_webview" />
        <activity android:name=".bookmark.MarkBrowserMainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".bookmark.BrowserActivity" />
        <activity android:name=".Memo.MemoMainActivity" />
        <activity android:name=".Memo.AppHelpActivity" />
        <activity android:name=".Memo.ModifyMyData" />
        <activity android:name=".Memo.ShowMyData" />
        <activity android:name=".Memo.WriteDiaryActivity" />
        <activity android:name=".Baro.BaroMainActivity" />
    </application>

</manifest>